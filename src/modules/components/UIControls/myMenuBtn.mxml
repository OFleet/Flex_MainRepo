<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark" xmlns:fx="http://ns.adobe.com/mxml/2009">
	<fx:Metadata>[HostComponent("spark.components.Button")]</fx:Metadata>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<s:BitmapImage smooth="true" source="@Embed('/assets/images/UpArrow.png')">
		<s:filters>
			<s:DropShadowFilter alpha="0.6" angle="30" distance="4" blurX="10" blurY="10"/>
		</s:filters>	
	</s:BitmapImage>
</s:Skin>
