<?xml version="1.0" encoding="utf-8"?>
<s:ItemRenderer xmlns:s="library://ns.adobe.com/flex/spark" xmlns:fx="http://ns.adobe.com/mxml/2009" autoDrawBackground="false">
	<s:states>
		<s:State name="normal"/>
		<s:State name="hovered"/>
		<s:State name="selected"/>
	</s:states>
	<s:RichText styleName="cityNames"  textDecoration="underline" trackingRight="-5%" whiteSpaceCollapse="preserve" text="{data}">
	</s:RichText>
	<s:Rect alpha.hovered="0.3"	alpha.normal="0" left="-6" right="-6" top="-2" bottom="-2"	alpha.selected="0.5" radiusX="12" radiusY="12">
		<s:fill>
			<s:SolidColor color="0xCED7EE"/>
		</s:fill>
	</s:Rect>
</s:ItemRenderer>
